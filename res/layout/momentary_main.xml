<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<TableLayout android:id="@+id/start_end_time_table"
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:stretchColumns="1" android:layout_below="@+id/time_of_occurance">

		<TableRow android:background = "#ff004400">
			<TextView android:id="@+id/TextView0111" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:gravity="center"
				android:textColor="#FFFFFF" android:text="Time" android:textSize="30sp"></TextView>

			<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_height="fill_parent" android:gravity="right|center"
				android:layout_width="wrap_content" android:paddingBottom="5px"
				android:paddingTop="5px" android:paddingRight="5px">
				<TextView android:id="@+id/TextView0222"
					android:layout_width="80px" android:layout_height="wrap_content"
					android:gravity="center" android:layout_marginLeft="10px" android:text=" Label"
					android:textSize="16sp"></TextView>

				<TextView android:id="@+id/TextView03333"
					android:layout_width="80px" android:layout_height="wrap_content"
					android:gravity="center" android:layout_marginLeft="20px" android:text="Inference"
					android:textSize="16sp"></TextView>

			</LinearLayout>
		</TableRow>
	</TableLayout>
	<ListView android:id="@+id/ListView01" android:layout_height="wrap_content"
		android:layout_width="fill_parent">
	</ListView>
</LinearLayout>
